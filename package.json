{
  "name": "svelte-project-template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite preview",
    "lint": "prettier --check src && tsc --noEmit && svelte-check --fail-on-warnings && eslint --max-warnings 0 src",
    "format": "eslint --fix src && prettier --write .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|ts|svelte)": [
      "eslint --max-warnings 0 --ext js --no-ignore"
    ],
    "*.(svelte|ts)": [
      "svelte-check --fail-on-warnings"
    ],
    "*.(ts|svelte|json|scss|md)": [
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-triple": "^1.1.3",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "typescript": "~5.2.2",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@babylonjs/core": "^6.19.1",
    "@babylonjs/gui": "^6.19.1",
    "@babylonjs/gui-editor": "^6.19.1",
    "@babylonjs/inspector": "^6.19.1",
    "@babylonjs/loaders": "^6.19.1",
    "@babylonjs/materials": "^6.19.1",
    "@babylonjs/serializers": "^6.19.1"
  }
}
