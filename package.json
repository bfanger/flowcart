{
  "name": "svelte-project-template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite preview",
    "lint": "prettier --check --loglevel=warn 'src/**/*.svelte' && tsc --noEmit && svelte-check --fail-on-warnings && eslint --max-warnings 0 src",
    "format": "prettier --write . && eslint --fix src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --max-warnings 0 --ext ts,tsx --no-ignore",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "*.js": [
      "eslint --max-warnings 0 --ext js --no-ignore"
    ],
    "*.svelte": [
      "svelte-check --fail-on-warnings",
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-triple": "^0.5.8",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.62.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  },
  "dependencies": {
    "@babylonjs/core": "^6.4.0",
    "@babylonjs/gui": "^6.4.0",
    "@babylonjs/gui-editor": "^6.4.0",
    "@babylonjs/inspector": "^6.4.0",
    "@babylonjs/loaders": "^6.4.0",
    "@babylonjs/materials": "^6.4.0",
    "@babylonjs/serializers": "^6.4.0"
  }
}
